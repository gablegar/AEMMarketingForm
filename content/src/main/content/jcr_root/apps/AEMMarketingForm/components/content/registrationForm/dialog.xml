<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
		  jcr:primaryType="cq:Dialog"
		  jcr:title="Registration Form Template"
		  xtype="tabpanel">
	<items jcr:primaryType="cq:WidgetCollection">
		<tab1 jcr:primaryType="cq:Panel"
			  title="Appearance">
			<items jcr:primaryType="cq:WidgetCollection">
				<customStylesheet
						jcr:primaryType="cq:Widget"
						fieldLabel="Campaign Name"
						name="./campaignName"
						options="/etc/acs-commons/lists/salesForceCampaignNames/_jcr_content.list.json"
						type="select"
						xtype="selection"/>
				<customStylesheetColor
						jcr:primaryType="cq:Widget"
						fieldLabel="Background Color"
						fieldDescription="chose the background color"
						name="./color"
						options="/etc/acs-commons/lists/backgroundColorList/_jcr_content.list.json"
						type="select"
						xtype="selection"/>
				<headerTitle
						jcr:primaryType="cq:Widget"
						fieldLabel="Enter form title"
						name="./title"
						xtype="textfield"/>
				<successTitle
						jcr:primaryType="cq:Widget"
						fieldLabel="Enter success message"
						name="./successMessage"
						xtype="textfield"/>
				<errorTitle
						jcr:primaryType="cq:Widget"
						fieldLabel="Enter success message"
						name="./errorMessage"
						xtype="textfield"/>
				<fieldListURL
						jcr:primaryType="cq:Widget"
						fieldLabel="Enter ACS generic field list url"
						name="./fieldsListUrl"
						xtype="textfield"/>
			</items>
		</tab1>
	</items>
</jcr:root>